{
    "extends" : "airbnb-base",
    "env": {
        "browser" : true,
        "es2021": true
    },
    "rules": {
        "linebreak-style" : 0,
        "global-require": 0,
        "eslint linebreak-style" : [0, "error", "windows"],
        "quotes": ["error", "double"],
        "no-console": "off",
        "no-alert": "off",
        "no-unused-vars": ["error", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
    }
}